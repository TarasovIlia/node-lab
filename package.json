{
  "name": "server",
  "version": "1.0.0",
  "description": "node server",
  "private": true,
  "scripts": {
    "start": "npm run dev && nodemon build/main.js",
    "dev": "cross-env MODE_ENV=development webpack --mode development",
    "build": "cross-env MODE_ENV=production webpack --mode production",
    "prettier": "prettier --config .prettierrc.json --write src/**/*.ts",
    "lint": "eslint src/**",
    "fix": "eslint --fix src/**"
  },
  "husky": {
    "hooks": {
      "pre-commit": "prety-quick --staged"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "config": "^3.3.6",
    "express": "^4.17.1",
    "pg": "^8.7.1",
    "pg-promise": "^10.11.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.37",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^16.9.1",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "clean-webpack-plugin": "^4.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "husky": "^7.0.2",
    "nodemon": "^2.0.12",
    "prettier": "^2.4.0",
    "pretty-quick": "^3.1.1",
    "ts-loader": "^9.2.5",
    "typescript": "^4.4.3",
    "webpack": "^5.52.1",
    "webpack-cli": "^4.8.0"
  }
}
